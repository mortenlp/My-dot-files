* Indsættes i /etc/profile:
  export PATH=/opt/texlive/2011/bin/i386-linux:$PATH
  export PATH=/opt/ilog2006/bin:$PATH

* Indsættes i .bashrc:
  alias imf="ssh -X -l mortenlp ssh1.imf.au.dk"
  alias ll="ls -l"
  alias pubWeb="scp ~/Dropbox/Dokumenter/Hjemmeside/index.html ~/Dropbox/Dokumenter/Hjemmeside/style.css ~/Dropbox/Dokumenter/Hjemmeside/menu.css ~/Dropbox/Dokumenter/Hjemmeside/skema.html mortenlp@ssh1.imf.au.dk:/home/mortenlp/public_html/"
  alias master="cd ~/Dropbox/Dokumenter/AU/MasterThesis/CutPlaneAlgo/trunk/ && scp cut_plane_alg.c cut_stuff.c cut_stuff.h splitcut.c splitcut.h lp_stuff.c lp_stuff.h Makefile-64BIT mortenlp@boromir.imf.au.dk:/home/mortenlp/Desktop/"
  export ILOG_LICENCE_FILE=/opt/ilog2006/ilm/access.ilm
