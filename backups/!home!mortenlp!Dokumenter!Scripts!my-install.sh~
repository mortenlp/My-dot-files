#!/bin/bash

echo "----------------------------------------"
echo "This will install your most wanted apps"
echo "----------------------------------------"
echo "Note: For it to work you must have the medibuntu repository enabled"
echo "----------------------------------------"
echo "emacs"
echo "subversion"
echo "git-core"
echo "w3m-el-snapshot"
echo "aspell-en"
echo "aspell-da"
echo "gnutls-bin"
echo "arandr"
echo "compizconfig-settings-manager"
echo "gimp"
echo "icedtea6-plugin"
echo "flashplugin-nonfree"
echo "non-free-codecs"
echo "ubuntu-restricted-extras"
echo "libdvdcss2"
echo "----------------------------------------"
sudo apt-get install emacs subversion git-core w3m-el-snapshot aspell-en aspell-da gnutls-bin arandr compizconfig-settings-manager vpnc openconnect gimp icedtea6-plugin flashplugin-nonfree non-free-codecs ubuntu-restricted-extras libdvdcss2