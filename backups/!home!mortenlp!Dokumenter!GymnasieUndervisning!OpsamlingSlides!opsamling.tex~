\section{Variabelsh.}

\subsection{}

\begin{frame}
  \frametitle{Variable}
  En variabel er karakteriseret ved tre ting:
  \pause
  \begin{itemize}[<+->]
  \item Variablen \emph{beskriver} et eller andet, fx
    \emph{temperaturen} i et køleskab.
  \item Man angiver variablen ved et \emph{symbol}, fx $T$
  \item Variable har et \emph{variationsområde} bestående af de tal
    der kan være værdier af variablen.
  \end{itemize}
\end{frame}

\begin{frame}
  \frametitle{Variable} Vi taler om to typer af variable:
  \emph{afhængige} og \emph{uafhængige} variable.
  \pause
  \begin{block}{Eksempel}
    Betragt følgende ligning:
    \begin{equation*}
      y= 2x^2 + 54
    \end{equation*}
    Da er $y$ den \emph{afhængige} variabel og $x$ er den \emph{uafhængige}
    variabel.
  \end{block}
\end{frame}

\begin{frame}\tiny
  \frametitle{Intervaller}
  \begin{table}[htbp]
    \centering
    \begin{tabular}[htbp]{l l r}
      \toprule
      Skrivemåde & Betydning & Illustration \\ \midrule
      $[a; b]$ & Mængden af tal mellem $a$ og $b$, begge inkl. & \includegraphics[scale=0.4]{lukket.png} \\
      $]a; b[$ & Mængden af tal mellem $a$ og $b$, begge ekskl. & \includegraphics[scale=0.4]{aaben.png} \\
      $]a; b]$ & Mængden af tal mellem $a$ og $b$, a ikke med, b med & \includegraphics[scale=0.4]{halvaaben.png} \\
      $[a; b[$ & Mængden af tal mellem $a$ og $b$, a med, b ikke med & \includegraphics[scale=0.4]{halvaaben2.png} \\ \bottomrule
    \end{tabular}
  \end{table}
\end{frame}

\section{Trigonometri}

\subsection{}

\begin{frame}
  \frametitle{Pythagoras sætning}
  \begin{block}{Pythagoras}
    For retvinklede trekanter gælder (med underforstået notation):
    \begin{equation*}
      a^2 + b^2 = c^2
    \end{equation*}
  \end{block}
\end{frame}

\begin{frame}
  \frametitle{Cosinus, sinus og tangens}
  \begin{center}
  \begin{tikzpicture}
    [scale=2,line cap=round
    % Styles
    axes/.style=,
    important line/.style={very thick},
    information text/.style={rounded corners,fill=red!10,inner sep=1ex}]
    % Local definitions
    \def\costhirty{0.8660256}
    % Colors
    \colorlet{anglecolor}{green!50!black}
    \colorlet{sincolor}{red}
    \colorlet{tancolor}{orange!80!black}
    \colorlet{coscolor}{blue}
    % The graphic
    \draw[help lines,step=0.5cm] (-1.4,-1.4) grid (1.4,1.4);
    \draw (0,0) circle (1cm);
    \draw[->] (-1.5,0) -- (1.5,0) node[right] {$x$} coordinate(x axis);
    \draw[->] (0,-1.5) -- (0,1.5) node[above] {$y$} coordinate(y axis);
    \foreach \x/\xtext in {-1, -.5/-\frac{1}{2}, 1}
    \draw[xshift=\x cm] (0pt,1pt) -- (0pt,-1pt) node[below,fill=white] {$\xtext$};
    \foreach \y/\ytext in {-1, -.5/-\frac{1}{2}, .5/\frac{1}{2}, 1}
    \draw[yshift=\y cm] (1pt,0pt) -- (-1pt,0pt) node[left,fill=white] {$\ytext$};
    \filldraw[fill=green!20,draw=anglecolor] (0,0) -- (3mm,0pt) arc(0:30:3mm);
    \draw (15:2mm) node[anglecolor] {$\alpha$};
    \draw[important line,sincolor]
    (30:1cm) -- node[left=1pt,fill=white] {$\sin \alpha$} (30:1cm |- x axis);
    \draw[important line,coscolor]
    (30:1cm |- x axis) -- node[below=2pt,fill=white] {$\cos \alpha$} (0,0);
    \draw[important line,tancolor] (1,0) -- node[right=1pt,fill=white] {
      $\displaystyle \tan \alpha \color{black}=
      \frac{{\color{sincolor}\sin \alpha}}{\color{coscolor}\cos \alpha}$}
    (intersection of 0,0--30:1cm and 1,0--1,1) coordinate (t);
    \draw (0,0) -- (t);
  \end{tikzpicture}
\end{center}
\end{frame}

\begin{frame}
  \frametitle{Nyttige formler}
  \begin{block}{Cosinus}
    \begin{align*}
      \cos(vinkel)= \frac{\text{hosliggende katete}}{\text{hypotenuse}}
    \end{align*}
  \end{block}
  \pause
  \begin{block}{Sinus}
    \begin{align*}
      \sin(vinkel) = \frac{\text{modstående katete}}{\text{hypotenuse}}
    \end{align*}
  \end{block}
  \pause
  \begin{block}{Tangens}
    \begin{align*}
      \tan(vinkel) = \frac{\text{modstående katete}}{\text{hosliggende katete}}
    \end{align*}
  \end{block}
\end{frame}

\begin{frame}
  \frametitle{Vilkårlige trekanter}
  \begin{block}{Sinusrelationen}
    For vilkårlige trekanter gælder (med underforstået notation)
    \begin{equation*}
      \frac{\sin A}{a} = \frac{\sin B}{b} = \frac{\sin C}{c}
    \end{equation*}
    eller ækvivalent
    \begin{equation*}
      \frac{a}{\sin A} = \frac{b}{\sin B} = \frac{c}{\sin C}
    \end{equation*}
  \end{block}
\end{frame}

\begin{frame}
  \frametitle{Vilkårlige trekanter}
  \begin{block}{Cosinusrelationen}
    For vilkårlige trekanter gælder:
    \begin{alignat*}{3}
      &a^2 = b^2 + c^2 -2bc \cdot \cos A \quad &\text{eller} \quad &
      \cos A = \frac{b^2 + c^2 - a^2}{2bc} \\
      &b^2 = a^2 + c^2 -2ac \cdot \cos B \quad &\text{eller} \quad &
      \cos B = \frac{a^2 + c^2 - b^2}{2ac} \\
      &c^2 = a^2 + b^2 -2ab \cdot \cos C \quad &\text{eller} \quad &
      \cos C = \frac{a^2 + b^2 - c^2}{2ab}
    \end{alignat*}
  \end{block}
\end{frame}

\section{Proportionalitet}

\subsection{}

\begin{frame}
  \frametitle{Ligefrem proportionalitet}
  \begin{block}{Ligefrem proportionalitet}
    Variablene $x$ og $y$ siges at være ligefrem proportionale hvis
    \begin{equation*}
      y= ax
    \end{equation*}
    Med andre ord
    \begin{equation*}
      \frac{y}{x}=a
    \end{equation*}
  \end{block}
\end{frame}

\begin{frame}
  \frametitle{Omvendt proportionalitet}
  \begin{block}{Omvendt proportionalitet}
    Variablene $x$ og $y$ siges at være omvendt proportionale hvis
    \begin{equation*}
      y= \frac{k}{x}
    \end{equation*}
    eller
    \begin{equation*}
      k= y \cdot x
    \end{equation*}
  \end{block}
\end{frame}

\begin{frame}
  \frametitle{Tilvækster}
  \begin{block}{Absolut tilvækst}
    Den absolutte tilvækst for variablen $x$ defineres som ændringen
    fra værdien $x_1$ til værdien $x_2$ dvs
    \begin{equation*}
      \Delta x= x_1 - x_2
    \end{equation*}
  \end{block}
  \pause
  \begin{block}{Relativ tilvækst}
    Den relative tilvækst defineres til at være den absolutte tilvækst
    divideret med startværdien.
    \begin{equation*}
      r= \frac{\Delta x}{x_1}
    \end{equation*}
  \end{block}
\end{frame}

\section{Vækstmodeller}

\subsection{}

\begin{frame}
  \frametitle{Lineære funktioner}
  Lineære funktioner er karakteriseret ved:
  \pause
  \begin{itemize}[<+->]
  \item Grafen er en ret linie (\emph{Definition})
  \item Regneforskriften er $f(x)= ax + b$
  \item De absolutte tilvækster er proportionale, dvs. $\Delta y= a
    \cdot \Delta x$
  \end{itemize}
\end{frame}

\begin{frame}
  \frametitle{Eksponentielle funktioner}
  Eksponentielle funktioner er karakteriseret ved:
  \pause
  \begin{itemize}[<+->]
  \item Regneforskriften er $f(x)= b \cdot a^x$, hvor $x$ er et reelt
    tal og $a$ og $b$ er positive tal (normalt er $a \neq 1$)
  \item Samme absolutte tilvækst i $x$-værdien giver samme relative
    vækst i $y$-værdien, dvs $y_2 = y_1 \cdot a^{\Delta x}$
  \item Grafen er en ret linie i et enkeltlogaritmisk koordinatsystem
  \end{itemize}
\end{frame}

\begin{frame}
  \frametitle{Fordoblings- og halveringskonstant}
  \begin{block}{Definition 1}
    Hvis den eksponentielle funktion $f$ er voksende, defineres
    fordoblingskonstanten, $T_2$, som den absolutte tilvækst i $x$ der
    giver en fordobling af $y$-værdien.
  \end{block}
  \pause
  \begin{block}{Definition 1$^{\prime}$}
    Hvis den eksponentielle funktion, $f$, er aftagende, defineres
    halveringskonstanten, $T_{\frac{1}{2}}$, som den absolutte
    tilvækst i $x$ der giver en halvering af $y$-værdien.
  \end{block}
\end{frame}

\begin{frame}
  \frametitle{Fordoblings- og halveringskonstant}
  \begin{block}{Sætning}
    Fordoblingskonstanten, $T_2$ for en voksende eksponentiel funktion
    kan beregnes ved formlen:
    \begin{equation*}
      T_2 = \frac{\log 2}{\log a}
    \end{equation*}
    Halveringskonstanten for en aftagende eksponentiel funktion kan
    beregnes ved formlen:
    \begin{equation*}
      T_{\frac{1}{2}} = \frac{\log \frac{1}{2}}{\log a}
    \end{equation*}
  \end{block}
\end{frame}

\begin{frame}
  \frametitle{Potensfunktioner}
  Potensfunktioner er kendetegnet ved:
  \pause
  \begin{itemize}[<+->]
  \item Regneforskriften er $f(x)= b \cdot x^a, \; x>0, \; b>0$
  \item Samme relative tilvækst i $x$ giver samme relative vækst i
    $y$: $f(x_0 \cdot F_x)= f(x_0) \cdot F_x^a$
  \item Grafen er en ret linie i et dobbeltlogaritmisk koordinatsystem.
  \end{itemize}
\end{frame}

\begin{frame}
  \frametitle{Regression}
  Lommeregner tricks:
  \pause
  \begin{itemize}[<+->]
  \item LinReg($ax +b$)
  \item ExpReg()
  \item PowerReg()
  \end{itemize}
\end{frame}

\begin{frame}
  \frametitle{Definitions- og værdimængde}
  \begin{block}{Definitionsmængde}
    Definitionsmængden for en funktion $f$ er defineret som den mængde
    af $x$-værdier der kan anvendes. Benævnes $Dm(f)$. For mange
    funktioner kan alle $x$-værdier bruges og man skriver så $Dm(f)=
    \mathbb{R}$
  \end{block}
  \pause
  \begin{block}{Værdimængde}
    Værdimængden er den mængde af $y$-værdier der fremkommer når $x$
    gennemløber hele definitionsmængden. Man skriver $Vm(f)$.
  \end{block}
  \pause
  \begin{block}{Spørgsmål}
    Hvad er værdimængden for funktionen $e^x$?
  \end{block}
\end{frame}

\section{2.g. polynomier}

\subsection{}

\begin{frame}
  \frametitle{Andengradspolynomier}
  \begin{block}{Definition}
    Et andengradspolynomium er en funktionstype med en regneforskrift
    af formen:
    \begin{equation*}
      f(x)= ax^2 + bx + c
    \end{equation*}
  \end{block}
\end{frame}

\begin{frame}
  \frametitle{Eksempel}
  \begin{center}
    \includegraphics[scale=0.4]{parabel.JPG}
  \end{center}
\end{frame}

\begin{frame}
  \frametitle{Mere om andengradspolynomier}
  \begin{block}{Toppunktsformlen}
    \begin{equation*}
      (x,y)= \bigg( \frac{-b}{2a}, \frac{-d}{4a}\bigg), \; \text{hvor} \; d= b^2 -4ac
    \end{equation*}
  \end{block}
  \pause
  \begin{block}{Rødder}
    Et tal $r$ kaldes en rod i et andengradspolynomium $f$, hvis
    $f(r)=0$
  \end{block}
  \pause
  \begin{block}{Rodformel}
    I et andengradspolynomium $f(x)= ax^2 + bx + c$ findes rødderne på
    følgende måde: Hvis $d\geq 0$ udregnes rod eller rødder ved formlen
    \begin{equation*}
      x= \frac{-b \pm \sqrt{d}}{2a}
    \end{equation*}
    Hvis $d < 0$ har andengradspolynomiet ingen rødder.
  \end{block}
\end{frame}

\begin{frame}
  \frametitle{Faktorisering og nulreglen}
  \begin{block}{Faktorisering}
    Hvis et andengradspolynomium $f$ har rødderne $x_1$ og $x_2$ kan det
    \emph{faktoriseres}. Der gælder nemlig:
    \begin{equation*}
      f(x)= ax^2 + bx + c = a(x-x_1)(x-x_2)
    \end{equation*}
  \end{block}
  \pause
  \begin{block}{Nulreglen}
    \begin{equation*}
      a \cdot b= 0 \Leftrightarrow a=0 \; \text{eller} \; b=0
    \end{equation*}
  \end{block}
\end{frame}

\begin{frame}
  \frametitle{Andengradsligninger}
  \begin{block}{Definition}
    En andengradsligning er en ligning på formen
    \begin{equation*}
      ax^2 + bx + c= 0
    \end{equation*}
    Denne form kaldes for andengradsligningens
    \emph{grundform}. Bemærk at ethvert andengradspolynomium kan
    bringes på denne form ved omformning.
  \end{block}
  \pause
  \begin{block}{Løsningsmetode}
    \begin{quote}
      Find rødder i det tilhørende andengradspolynomium!!!
    \end{quote}
  \end{block}
\end{frame}

\section{Differentialregning}

\subsection{}

\begin{frame}
  \frametitle{Differentialkoeficient}
  \begin{center}
    \includegraphics[scale=0.4]{diff.png}
  \end{center}
\end{frame}

\begin{frame}
  \frametitle{Differentialkoeficient}
  \begin{block}{Definition}
    \begin{equation*}
      f^{\prime}(x)= \lim_{h \to 0} \frac{f(x+h) - f(x)}{h}
    \end{equation*}
  \end{block}
\end{frame}

\begin{frame}
  \frametitle{Tretrinsreglen}
  \begin{block}{Første trin}
    Udregn $f(x_0 + h)$, hvor $h=\Delta x= x -x_0$. Udregn desuden
    $\Delta f$ som er givet ved
    \begin{equation*}
      \Delta f= y - y_0= f(x_0 + h) - f(x_0)
    \end{equation*}
  \end{block}
  \pause
  \begin{block}{Andet trin}
    Udregn sekanthældningen:
    \begin{equation*}
      a= \frac{\Delta f}{\Delta x} = \frac{\Delta f}{h}= \frac{f(x_0 + h) - f(x_0)}{h}
    \end{equation*}
  \end{block}
  \pause
  \begin{block}{Tredje trin}
    Hvis der er en grænseværdi så find den!
    \begin{equation*}
      f^{\prime}(x_0)= \lim_{h \to 0} \frac{\Delta f}{h}
    \end{equation*}
  \end{block}
\end{frame}

\begin{frame}
  \frametitle{Tangentens ligning}
  \begin{block}{Tangentens ligning}
    \begin{equation*}
      y= f(x_0) + f^{\prime}(x_0)(x-x_0)
    \end{equation*}
  \end{block}
\end{frame}

\begin{frame}
  \frametitle{Anvendelser}
  \begin{itemize}[<+->]
  \item Monotoniforhold og lokale ekstrema
  \item Bestemmelse af parablens toppunkt
  \item Optimering
  \end{itemize}
\end{frame}

\section{Integralregning}

\subsection{}

\begin{frame}
  \frametitle{Stamfunktioner}
  \begin{block}{Definition}
    $F$ kaldes en stamfunktion til funktionen $f$ hvis $F$ er
    differentiabel, og der gælder at $F^{\prime}(x)= f(x)$. Ofte skrives
    \begin{equation*}
      F(x)= \int f(x)\text{dx}
    \end{equation*}
  \end{block}
\end{frame}

\begin{frame}
  \frametitle{Arealberegning vha. integraler}
  \begin{block}{Arealberegning}
    Hvis funktionen $f$ er kontinuert og positiv i et interval
    $[a;b]$, kan arealet mellem grafen for $f$ og $x$-aksen afgrænset
    af tallene $a$ og $b$ beregnes ved:
    \begin{equation*}
      \int_a^b f(x) \text{dx} = [F(x)]_a^b = F(b)-F(a)
    \end{equation*}

  \end{block}
\end{frame}


